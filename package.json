{
  "name": "wasm-cpp-ar-effect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write '*/**/*.{ts,tsx,css,md,json}'",
    "lint:ts": "eslint --ext .ts,.tsx src",
    "lint:css": "stylelint '**/*.{ts,tsx}'",
    "prepare:husky": "husky install",
    "lint-staged": "lint-staged",
    "install:emsdk": "cd externals/emsdk && ./emsdk install latest && ./emsdk activate latest",
    "build:wasm": "cd cpp && mkdir -p build && cd build && ../../externals/emsdk/upstream/emscripten/emcmake cmake .. && ../../externals/emsdk/upstream/emscripten/emmake make && mv hello.wasm ../../public/"
  },
  "dependencies": {
    "@types/node": "20.1.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "next": "13.4.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@linters/commitlint-config": "^1.8.0",
    "@linters/eslint-config-jest": "^1.12.1",
    "@linters/eslint-config-react": "^1.14.0",
    "@linters/eslint-config-typescript": "^1.18.0",
    "@linters/prettier-config": "^1.4.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  }
}
